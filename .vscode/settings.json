{
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "editor.detectIndentation": false,
    "editor.tabSize": 4,
    "editor.insertSpaces": true,
    "git.blame.editorDecoration.enabled" : false,
    "python.analysis.autoImportCompletions": false,
    "python.defaultInterpreterPath" : "venv/bin/python3",
    "[python]": {
        "editor.defaultFormatter": "ms-python.autopep8",
        "editor.formatOnSave": true
    },
    "python.analysis.extraPaths": ["stubs"],
    "[docker]": {
        "editor.formatOnSave": false
    },
    "autopep8.args": [
        "--max-line-length",
        "150",
        "--select",
        "E101,E11,E121,E122,E123,E124,E125,E126,E127,E128,E129,E131,E133,E20,E211,E22,E224,E225,E226,E227,E228,E231,E241,E242,E251,E252,E26,E265,E27,E301,E302,E303,E304,E305,E306,E401,E501,E502,W291,W292,W293,W391,W503,W504,W603 "
    ],
    "cSpell.words": [
        "aliasdef",
        "appauthor",
        "appdirs",
        "baudrate",
        "bitmask",
        "bitoffset",
        "bitsize",
        "colmap",
        "databits",
        "Datalog",
        "dataseries",
        "deinterleaved",
        "Endianness",
        "endregion",
        "ephemerous",
        "firmwareid",
        "lastrowid",
        "Loggable",
        "msgbox",
        "pollable",
        "reqid",
        "reqids",
        "rpvs",
        "scrutinydebugger",
        "stopbits",
        "subfn",
        "testcases",
        "typehints",
        "varbit",
        "xaxis"
    ],
    "cSpell.enabled": true,
    "auto-correct.dictionary": [
        {
            "languages": ["*"],
            "words": {
                "backgorund": "background",
                "downlaod": "download",
                "scurtiny": "scrutiny",
                "isntall": "install",
                "isntalled": "installed",
                "is_isntalled": "is_installed",
                "laoded" : "loaded",
                "conenction" : "connection",
                "conencted" : "connected",
                "lsitener" : "listener",
                "lsiten" : "listen",
                "lsitening" : "listening",
                "compelte" : "complete",
                "compelted" : "completed"
            }
        }]
}
